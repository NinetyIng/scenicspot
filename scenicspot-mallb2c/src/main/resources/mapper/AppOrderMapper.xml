<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ythd.ower.b2c.mapper.AppProductStockMapper">

    <resultMap id="StockReturn" type="com.ythd.ower.b2c.model.ProductOrderModel">
        <id property="id" column="order_id" jdbcType="INTEGER" ></id>
        <result property="orderSn" column="order_sn" jdbcType="VARCHAR"></result>
        <result property="userId" column="user_id" jdbcType="INTEGER"></result>
        <result property="userName" column="user_name" jdbcType="VARCHAR"></result>
        <result property="orderMoney" column="order_money" jdbcType="DECIMAL"></result>
        <result property="goodsMoney" column="goods_money" jdbcType="DECIMAL"></result>
        <result property="refundMoney" column="refund_money" jdbcType="DECIMAL"></result>
        <result property="orderStatus" column="order_status" jdbcType="VARCHAR"></result>
        <result property="shippingStatus" column="shipping_status" jdbcType="VARCHAR"></result>
        <result property="shippingType" column="shipping_type" jdbcType="VARCHAR"></result>
        <result property="payStatus" column="pay_status" jdbcType="VARCHAR"></result>
        <result property="payType" column="pay_type" jdbcType="VARCHAR"></result>
        <result property="contactName" column="contact_name" jdbcType="VARCHAR"></result>
        <result property="contactPhone" column="contact_phone" jdbcType="VARCHAR"></result>
        <result property="provinceId" column="province_id" jdbcType="VARCHAR"></result>
        <result property="province" column="province" jdbcType="VARCHAR"></result>
        <result property="cityId" column="city_id" jdbcType="VARCHAR"></result>
        <result property="city" column="city" jdbcType="VARCHAR"></result>
        <result property="areaId" column="area_id" jdbcType="VARCHAR"></result>
        <result property="area" column="area" jdbcType="VARCHAR"></result>
        <result property="address" column="address" jdbcType="VARCHAR"></result>
        <result property="userNote" column="user_note" jdbcType="VARCHAR"></result>
        <result property="createTime" column="create_time" jdbcType="VARCHAR"></result>
        <result property="payTime" column="pay_time" jdbcType="VARCHAR"></result>
        <result property="signTime" column="sign_time" jdbcType="VARCHAR"></result>
        <result property="sendTime" column="send_time" jdbcType="VARCHAR"></result>
        <result property="mer" column="is_mer" jdbcType="INTEGER"></result>
        <result property="merType" column="mer_type" jdbcType="VARCHAR"></result>
        <result property="merTitle" column="mer_title" jdbcType="VARCHAR"></result>
        <result property="merNo" column="mer_no" jdbcType="VARCHAR"></result>
        <result property="delete" column="is_delete" jdbcType="INTEGER"></result>
        <result property="createType" column="create_type" jdbcType="VARCHAR"></result>
        <result property="refundResult" column="refund_result" jdbcType="VARCHAR"></result>
        <result property="failReason" column="fail_reason" jdbcType="VARCHAR"></result>
    </resultMap>

    <insert id="createOrder" parameterType="com.ythd.ower.b2c.model.ProductOrderModel">
        INSERT  INTO t_orders (order_sn,user_id,user_name,order_money,goods_money,order_status,shipping_status,shipping_type,pay_status,
        pay_type,contact_name,contact_phone,province_id,province,city_id,city,area_id,area,address,
        user_note,create_time,is_mer,mer_type,mer_title,mer_no,is_delete,create_type)
        VALUES (#{orderSn},#{userId},#{userName},#{orderMoney},#{goodsMoney},#{orderStatus},#{shippingStatus},#{shippingType},#{payStatus},
        #{payType},#{contactName},#{contactPhone},#{provinceId},#{province},#{cityId},#{city},#{areaId},#{area},#{address},
        #{userNote},#{createTime},#{mer},#{merType},#{merTitle},#{merNo},#{delete},#{createType})
        <selectKey keyProperty="id" resultType="Long">
            select LAST_INSERT_ID() as id
        </selectKey>
    </insert>


</mapper>